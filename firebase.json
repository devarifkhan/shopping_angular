{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting":{
    "public":"dist/shopping_app/browser",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}